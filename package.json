{
  "name": "@tokenized/sdk-example-react",
  "author": {
    "name": "Tokenized",
    "email": "admin@tokenized.com",
    "url": "https://tokenized.com/"
  },
  "version": "0.3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tokenized/tokenized-sdk-example-react.git"
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@reduxjs/toolkit": "^1.5.1",
    "@tokenized/sdk-js-private": "^0.3.0",
    "@tokenized/sdk-react-private": "^0.3.0",
    "bulma": "^0.9.2",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.18.0",
    "react-query": "^3.16.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "extract-strings": "formatjs extract 'src/**/*.js' --out-file src/lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile-strings": "formatjs compile-folder --ast src/lang src/compiled-lang",
    "generate-docs": "jsdoc2md --template docs/README.template.md node_modules/@tokenized/sdk-react-private/src/*.js node_modules/@tokenized/sdk-js-private/src/index.js node_modules/@tokenized/sdk-js-private/src/tokenized-api/*.js > README.md && prettier --write ./README.md"
  },
  "eslintConfig": {
    "root": true,
    "extends": "react-app",
    "plugins": [
      "formatjs"
    ],
    "rules": {
      "formatjs/enforce-id": [
        "error",
        {
          "idInterpolationPattern": "[sha512:contenthash:base64:6]"
        }
      ]
    }
  },
  "browserslist": [
    "> 1.5%",
    "not dead"
  ],
  "devDependencies": {
    "@formatjs/cli": "^4.2.15",
    "eslint-plugin-formatjs": "^2.15.2",
    "jsdoc-to-markdown": "^7.0.1",
    "prettier": "^2.2.1"
  }
}
