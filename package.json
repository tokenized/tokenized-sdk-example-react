{
  "name": "@tokenized/sdk-example-react",
  "author": {
    "name": "Tokenized",
    "email": "admin@tokenized.com",
    "url": "https://tokenized.com/"
  },
  "version": "0.6.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tokenized/tokenized-sdk-example-react.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@reduxjs/toolkit": "^1.8.0",
    "@tokenized/sdk-js-private": "^0.6.1",
    "@tokenized/sdk-react-private": "^0.6.1",
    "bulma": "^0.9.3",
    "classnames": "^2.3.1",
    "downshift": "^6.1.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.8",
    "react-final-form-listeners": "^1.0.3",
    "react-intl": "^5.24.7",
    "react-query": "^3.34.16",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.config.js --mode development --progress --hot",
    "build": "webpack --mode=production --config webpack.config.js",
    "update-strings": "formatjs extract 'src/**/*.js' --out-file src/lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' && npm run compile-translations",
    "compile-translations": "formatjs compile-folder --ast src/lang src/lang-compiled",
    "generate-files": "npm run update-strings",
    "format-all": "prettier --write .",
    "lint": "eslint .",
    "lint-fix": "eslint --fix ."
  },
  "eslintConfig": {
    "root": true,
    "plugins": [
      "formatjs"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:promise/recommended",
      "plugin:compat/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "prettier"
    ],
    "rules": {
      "formatjs/enforce-default-message": [
        "error",
        "literal"
      ],
      "formatjs/no-id": [
        "error"
      ],
      "formatjs/no-camel-case": [
        "error"
      ],
      "formatjs/no-emoji": [
        "error"
      ],
      "formatjs/no-multiple-whitespaces": [
        "error"
      ],
      "formatjs/no-multiple-plurals": [
        "error"
      ],
      "formatjs/no-offset": [
        "error"
      ],
      "react/prop-types": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": [
    "> 1.5%",
    "not dead"
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@formatjs/cli": "^4.8.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "babel-loader": "^8.2.4",
    "babel-plugin-formatjs": "^10.3.18",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.1",
    "eslint-plugin-formatjs": "^3.0.0",
    "eslint-plugin-react": "^7.29.4",
    "html-webpack-plugin": "^5.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "prettier": "^2.6.0",
    "process": "^0.11.10",
    "react-refresh": "^0.11.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "util": "^0.12.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
